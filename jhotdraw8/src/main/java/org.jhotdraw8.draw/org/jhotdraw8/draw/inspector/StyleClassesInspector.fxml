<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import java.lang.String?>
<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/10.0.1"
            xmlns:fx="http://javafx.com/fxml/1">
  <bottom>
    <HBox spacing="4.0" BorderPane.alignment="CENTER">
      <children>
        <Button fx:id="addButton" mnemonicParsing="false" text="%addButton.text">
          <tooltip>
            <Tooltip text="%addButton.toolTipText"/>
          </tooltip>
          <HBox.margin>
            <Insets/>
          </HBox.margin>
        </Button>
        <Button fx:id="removeButton" mnemonicParsing="false" text="%removeButton.text">
          <tooltip>
            <Tooltip text="%removeButton.toolTipText"/>
          </tooltip>
          <HBox.margin>
            <Insets/>
          </HBox.margin>
        </Button>
        <TextField fx:id="textField" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                   HBox.hgrow="ALWAYS"/>
      </children>
      <BorderPane.margin>
        <Insets bottom="4.0" left="4.0" right="4.0" top="4.0"/>
      </BorderPane.margin>
    </HBox>
  </bottom>
  <center>
    <ListView fx:id="listView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="44.0"
              minWidth="44.0" styleClass="flushTopRightLeft" BorderPane.alignment="CENTER"/>
  </center>
  <styleClass>
    <String fx:value="inspector"/>
    <String fx:value="flush"/>
  </styleClass>
</BorderPane>
